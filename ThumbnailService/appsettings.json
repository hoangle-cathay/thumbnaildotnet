{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "CloudSqlPostgres": "Host=/cloudsql/hoangassignment:asia-southeast1:leo-cloudsql-postgre;Database=thumbnaildb;Username=appuser;Password={PLACEHOLDER}"
  },
  "Gcp": {
    "ProjectId": "hoangassignment",
    "KmsKeyResource": "projects/hoangassignment/locations/asia-southeast1/keyRings/thumbnail-keyring/cryptoKeys/thumbnail-key/cryptoKeyVersions/1",
    "OriginalsBucket": "leo-originals-bucket",
    "ThumbnailsBucket": "leo-thumbnails-bucket",
    "CompressedPrefix": "compressed/",
    "UseSignedUrls": true,
    "SignedUrlExpiryMinutes": 60
  },
  "Thumbnail": {
    "Width": 100,
    "Height": 100,
    "Crop": true,
    "MaxRetries": 3
  },
  "Security": {
    "AesKeySecretName": "projects/PROJECT_ID/secrets/AES_KEY/versions/latest",
    "AesIvSecretName": "projects/PROJECT_ID/secrets/AES_IV/versions/latest"
  },
  "AllowedHosts": "*"
}
